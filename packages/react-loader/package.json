{
  "name": "@sanity/react-loader",
  "version": "1.10.35",
  "homepage": "https://github.com/sanity-io/visual-editing/tree/main/packages/react-loader#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/visual-editing/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/visual-editing.git",
    "directory": "packages/react-loader"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "deno": "./dist/index.js",
      "edge": "./dist/index.js",
      "edge-light": "./dist/index.js",
      "worker": "./dist/index.js",
      "react-server": "./dist/rsc/index.react-server.js",
      "browser": {
        "source": "./src/index.browser.ts",
        "require": "./dist/index.browser.cjs",
        "import": "./dist/index.browser.js"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./jsx": {
      "source": "./src/jsx/index.ts",
      "import": "./dist/jsx.js",
      "require": "./dist/jsx.cjs",
      "default": "./dist/jsx.js"
    },
    "./rsc": {
      "source": "./src/rsc/index.ts",
      "react-server": "./dist/rsc/index.react-server.js",
      "import": "./dist/rsc/index.js",
      "require": "./dist/rsc/index.cjs",
      "default": "./dist/rsc/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "jsx": [
        "./dist/jsx.d.ts"
      ],
      "rsc": [
        "./dist/rsc/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "pkg build --strict --check --clean",
    "dev": "pkg build --strict",
    "lint": "eslint --cache .",
    "prepack": "turbo run build",
    "test": "vitest --pass-with-no-tests --typecheck"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "prettier": "@repo/prettier-config",
  "eslintConfig": {
    "extends": [
      "@repo/eslint-config"
    ],
    "root": true
  },
  "dependencies": {
    "@sanity/client": "^6.24.3",
    "@sanity/core-loader": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/package.config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/visual-editing-helpers": "workspace:*",
    "@sanity/pkg-utils": "6.13.4",
    "@types/react": "^19.0.7",
    "eslint": "^8.57.1",
    "fast-deep-equal": "3.1.3",
    "react": "^19.0.0",
    "typescript": "5.7.3",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "react": "^18.3 || >=19.0.0-rc"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
