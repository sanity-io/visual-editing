{
  "name": "@repo/sanity-schema",
  "version": "0.0.0",
  "private": true,
  "description": "Shared Sanity schema used by the Studio app, as well as for TypeGen in the other apps",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --cache ."
  },
  "browserslist": "extends @sanity/browserslist-config",
  "prettier": "@repo/prettier-config",
  "eslintConfig": {
    "extends": [
      "@repo/eslint-config"
    ],
    "root": true
  },
  "dependencies": {
    "@repo/env": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@sanity/assist": "3.0.6",
    "@sanity/color-input": "4.0.1",
    "@sanity/presentation": "workspace:*",
    "eslint": "^8.57.1",
    "rxjs": "^7.8.1",
    "sanity": "3.57.3",
    "sanity-plugin-asset-source-unsplash": "3.0.1",
    "typescript": "5.6.2"
  },
  "peerDependencies": {
    "@sanity/assist": "$@sanity/assist",
    "@sanity/color-input": "$@sanity/color-input",
    "@sanity/presentation": "$@sanity/presentation",
    "rxjs": "$rxjs",
    "sanity": "$sanity",
    "sanity-plugin-asset-source-unsplash": "$sanity-plugin-asset-source-unsplash"
  }
}
