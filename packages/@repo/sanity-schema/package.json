{
  "name": "@repo/sanity-schema",
  "version": "0.0.0",
  "private": true,
  "description": "Shared Sanity schema used by the Studio app, as well as for TypeGen in the other apps",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pkg build --strict --check --clean",
    "lint": "eslint .",
    "prepack": "turbo run build"
  },
  "dependencies": {
    "@repo/env": "workspace:*",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/package.config": "workspace:*",
    "@sanity/assist": "catalog:",
    "@sanity/color-input": "catalog:",
    "@sanity/icons": "catalog:",
    "@sanity/pkg-utils": "catalog:",
    "@sanity/preview-url-secret": "workspace:^",
    "@sanity/tsconfig": "catalog:",
    "@sanity/ui": "catalog:",
    "@sanity/visual-editing-csm": "workspace:*",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "^9.0.0",
    "rxjs": "catalog:",
    "sanity": "catalog:",
    "sanity-plugin-asset-source-unsplash": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@sanity/assist": "catalog:",
    "@sanity/color-input": "catalog:",
    "@sanity/icons": "catalog:",
    "@sanity/ui": "catalog:",
    "rxjs": "catalog:",
    "sanity": "catalog:",
    "sanity-plugin-asset-source-unsplash": "catalog:"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "eslintConfig": {
    "extends": [
      "@repo/eslint-config"
    ],
    "root": true
  }
}
