{
  "name": "@sanity/groq-store",
  "version": "3.1.6",
  "description": "Query Content Lake with GROQ. Fetch once for SSR, or listen to changes in real-time.",
  "keywords": [
    "groq",
    "live",
    "memory",
    "preview",
    "query",
    "react",
    "sanity.io",
    "sanity",
    "sanity",
    "studio",
    "svelte",
    "vue"
  ],
  "homepage": "https://github.com/sanity-io/groq-store#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/groq-store/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/groq-store.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf coverage dist",
    "build": "pkg build --strict && pkg --strict",
    "watch": "pnpm build watch",
    "coverage": "vitest run --coverage",
    "lint": "eslint --max-warnings 0 .",
    "prepublishOnly": "pnpm run build",
    "test": "vitest"
  },
  "browserslist": [
    "> 0.2% and supports es6-module and supports es6-module-dynamic-import and not dead and not IE 11",
    "maintained node versions"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@nanostores/query": "0.2.4",
    "lru-cache": "10.0.1",
    "mendoza": "3.0.3",
    "nanostores": "0.9.3"
  },
  "devDependencies": {
    "@sanity/client": "^6.4.12",
    "@sanity/pkg-utils": "^2.4.8",
    "@types/node": "^18.17.15",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitest/coverage-v8": "^0.34.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-gitignore": "^0.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "peerDependencies": {
    "@sanity/client": "^6.4.12"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "restricted",
    "tag": "canary",
    "provenance": false
  }
}
