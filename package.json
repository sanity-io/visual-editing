{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=./apps/*",
    "format": "prettier --cache --write .",
    "lint": "turbo run lint",
    "lint:fix": "pnpm lint -- --fix",
    "preview": "turbo run preview",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "watch": "turbo run watch"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson",
      "prettier-plugin-svelte",
      "prettier-plugin-tailwindcss"
    ],
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@sanity/block-tools": "3.18.1-pink-lizard.17",
    "@sanity/cli": "3.18.1-pink-lizard.17",
    "@sanity/diff": "3.18.1-pink-lizard.17",
    "@sanity/export": "3.18.1-pink-lizard.17",
    "@sanity/import": "3.18.1-pink-lizard.17",
    "@sanity/import-cli": "3.18.1-pink-lizard.17",
    "@sanity/mutator": "3.18.1-pink-lizard.17",
    "@sanity/portable-text-editor": "3.18.1-pink-lizard.17",
    "@sanity/schema": "3.18.1-pink-lizard.17",
    "@sanity/types": "3.18.1-pink-lizard.17",
    "@sanity/util": "3.18.1-pink-lizard.17",
    "@sanity/vision": "3.18.1-pink-lizard.17",
    "create-sanity": "3.18.1-pink-lizard.17",
    "groq": "3.18.1-pink-lizard.17",
    "sanity": "3.18.1-pink-lizard.17"
  },
  "devDependencies": {
    "prettier": "3.0.3",
    "prettier-plugin-packagejson": "2.4.6",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "turbo": "1.10.15"
  },
  "packageManager": "pnpm@8.9.0",
  "pnpm": {
    "overrides": {
      "@sanity/block-tools": "$@sanity/block-tools",
      "@sanity/cli": "$@sanity/cli",
      "@sanity/diff": "$@sanity/diff",
      "@sanity/export": "$@sanity/export",
      "@sanity/import": "$@sanity/import",
      "@sanity/import-cli": "$@sanity/import-cli",
      "@sanity/mutator": "$@sanity/mutator",
      "@sanity/portable-text-editor": "$@sanity/portable-text-editor",
      "@sanity/schema": "$@sanity/schema",
      "@sanity/types": "$@sanity/types",
      "@sanity/util": "$@sanity/util",
      "@sanity/vision": "$@sanity/vision",
      "create-sanity": "$create-sanity",
      "groq": "$groq",
      "sanity": "$sanity"
    }
  }
}
