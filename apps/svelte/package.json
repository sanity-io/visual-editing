  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.2.7",
    "@repo/sanity-extracted-schema": "workspace:*",
    "@sanity/client": "catalog:",
    "@sanity/preview-url-secret": "workspace:*",
    "@sanity/svelte-loader": "workspace:*",
    "@sanity/sveltekit": "^0.0.1",
    "@sanity/visual-editing": "workspace:*",
    "@sanity/visual-editing-csm": "workspace:*",
    "@sveltejs/adapter-auto": "^6.1.1",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/cookie": "^0.6.0",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "eslint": "^8.57.1",
    "eslint-plugin-svelte": "^2.46.1",
    "postcss": "8.5.6",
    "svelte": "^5.49.1",
    "svelte-check": "^4.3.5",
    "tailwindcss": "catalog:tailwind3",
    "tslib": "^2.8.1",
    "typescript": "catalog:",
    "vite": "^7.3.1"
  }
